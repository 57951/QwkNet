# EditorConfig is awesome: https://EditorConfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.cs]
indent_style = space
indent_size = 2

[*.csproj]
indent_style = space
indent_size = 2

[*.json]
indent_style = space
indent_size = 2

# British English spelling
spelling_languages = en-GB
